    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: jsonlogalert pick args",
            "type": "debugpy",
            "request": "launch",
            "module": "jsonlogalert",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python Debugger: jsonlogalert",
            "type": "debugpy",
            "request": "launch",
            "module": "jsonlogalert",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": ["--print-rules", "-vvv"]
        }
    ]
}
